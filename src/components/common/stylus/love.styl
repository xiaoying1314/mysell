@font-face {
  font-family: 'sell-icon';
  src:  url('fonts/sell-icon.eot?2la5be');
  src:  url('fonts/sell-icon.eot?2la5be#iefix') format('embedded-opentype'),
    url('fonts/sell-icon.ttf?2la5be') format('truetype'),
    url('fonts/sell-icon.woff?2la5be') format('woff'),
    url('fonts/sell-icon.svg?2la5be#sell-icon') format('svg');
  font-weight: normal;
  font-style: normal;
  font-display: block;
}

[class^="icon-"], [class*=" icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'sell-icon' !important;
  speak: never;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
